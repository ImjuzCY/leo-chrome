{
  "name": "LEO Address Bar Search",
  "description": "Adds a 'leo' command to the address bar to quickly look up words in the LEO dictionary.",
  "version": "1.2",
  "manifest_version": 3,
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "omnibox": {
    "keyword": "leo"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "options_page": "html/options.html"
}
